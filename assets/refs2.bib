	Issn = {0002-9637},
	Journal = {The American journal of tropical medicine and hygiene},
	Keywords = {Age Distribution,Cause of Death,Child, Preschool,Demography,Government Agencies,Humans,Infant,Infant Mortality,Infant, Newborn,Population Surveillance,Population Surveillance: methods,Questionnaires,Registries,Rural Population,Time Factors, Sam},
	Month = apr,
	Number = {4 Suppl},
